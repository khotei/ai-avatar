  "dependencies": {
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2"
  },
  "devDependencies": {
    "@ai-avatar/lint": "*",
    "@types/node": "^22.15.17",
    "concurrently": "^8.2.2"
  }
}

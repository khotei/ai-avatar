{
  "name": "@ai-avatar/env-config",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "type-check": "tsc -b --noEmit",
    "lint": "lint",
    "format": "lint --fix",
    "clean": "rm -rf ./node_modules && rm -rf ./.turbo && rm -rf ./dist"
  },
  "dependencies": {
    "lodash": "^4.17.16",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@ai-avatar/lint": "*",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.17"
  }
}

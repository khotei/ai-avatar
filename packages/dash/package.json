{
  "name": "@ai-avatar/dash",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "type-check": "tsc -b --noEmit",
    "lint": "lint",
    "format": "lint --fix",
    "clean": "rm -rf ./node_modules && rm -rf ./.turbo && rm -rf ./dist"
  },
  "dependencies": {
    "lodash": "^4.17.16",
    "lodash-es": "^4.17.21",
    "type-fest": "^4.41.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@ai-avatar/env-config": "*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.17",
    "concurrently": "^8.2.2"
  }
}

{
  "compilerOptions": {
    "target": "ES2024",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "incremental": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
  }
}